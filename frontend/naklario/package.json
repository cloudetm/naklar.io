{
  "name": "naklario",
  "version": "0.0.0",
  "scripts": {
    "android": "tns run android --env.aot",
    "ios": "tns run ios --env.aot",
    "mobile": "tns run",
    "preview": "tns preview",
    "ngcc": "ngcc --properties es2015 module main --first-only",
    "postinstall": "npm run ngcc",
    "ng": "ng",
    "start": "nice -10 ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "dev:ssr": "nice -10 ng run naklario:serve-ssr",
    "serve:ssr": "nice -10 node dist/naklario/server/main.js",
    "build:ssr": "ng build --prod && ng run naklario:server:production",
    "prerender": "ng run naklario:prerender"
  },
  "private": true,
  "dependencies": {
    "@nativescript/angular": "~9.0.0",
    "@nativescript/core": "~6.5.5",
    "@nativescript/theme": "~2.2.1",
    "reflect-metadata": "~0.1.12",
    "tslib": "^1.10.0",
    "@angular/animations": "~9.1.9",
    "@angular/common": "~9.1.9",
    "@angular/compiler": "~9.1.9",
    "@angular/core": "~9.1.9",
    "@angular/forms": "^9.1.9",
    "@angular/localize": "^9.1.9",
    "@angular/platform-browser": "^9.1.9",
    "@angular/platform-browser-dynamic": "~9.1.9",
    "@angular/platform-server": "~9.1.9",
    "@angular/router": "~9.1.9",
    "@nativescript/schematics": "^9.0.0",
    "@ng-bootstrap/ng-bootstrap": "^6.0.2",
    "@ng-select/ng-select": "^4.0.0",
    "@nguniversal/express-engine": "^9.1.1",
    "@ngx-loading-bar/core": "^4.2.0",
    "@ngx-loading-bar/http-client": "^4.2.0",
    "@ngx-loading-bar/router": "^4.2.0",
    "bootstrap": "^4.4.1",
    "bootswatch": "^4.4.1",
    "express": "^4.17.1",
    "http-proxy-middleware": "^1.0.3",
    "material-design-icons": "^3.0.1",
    "ng5-slider": "^1.2.4",
    "rxjs": "~6.5.5",
    "zone.js": "~0.10.2"
